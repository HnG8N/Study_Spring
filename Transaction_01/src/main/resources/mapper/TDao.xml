<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springlec.base.dao.TDao">

	<insert id="writeDao">
		INSERT INTO PRODUCT(id, product) VALUES (#{id}, #{product})
	</insert>
	
	<insert id="writeDao2">
		INSERT INTO POINT(ad, point) VALUES (#{id}, 10)
	</insert>
	
</mapper>