package com.springlec.base.service.admin;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.springlec.base.dao.admin.LoginVerification_Dao;
import com.springlec.base.model.admin.LoginVerification_Dto;

@Service
public class LoginVerification_ServiceImpl implements LoginVerification_Service{

	@Autowired
	LoginVerification_Dao dao;
	
	@Override
	public LoginVerification_Dto loginVerification(String mid) throws Exception {
		LoginVerification_Dto loginVerification = dao.loginVerification(mid);
		return loginVerification;
	}

}
